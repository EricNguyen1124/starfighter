#include main.lua

function _init()
  star = {}
  star.x = 8
  star.y = 25
  star.z = 8

  pl={}
	pl.x = 0 pl.y = 0
	pl.dx = 0 pl.dy = 0
	pl.z = 0
	pl.d = 0.0
	pl.dz = 0
	pl.jetpack=false

	x = 64
  y = 64
end

function _update()
  if (btn(0)) then x=x-1 end
  if (btn(1)) then x=x+1 end
  if (btn(2)) then y=y-1 end
  if (btn(3)) then y=y+1 end

  local dx=0
	local dy=0
  if (btn(⬆️)) dy+= 1
	if (btn(⬇️)) dy-= 1

  star.z -= 5
end
 
function _draw()
  cls(5)
  circfill(x,y,7,14)
end
